#include <stdio.h>
#include <unistd.h>
#include <sys/types.h>
#include <sys/wait.h>
 /**
  * main - fork example
  *
  * Return: Always 0.
  */
int main(void)
{
	char *argv[] = {"/bin/ls", "-l", "./", NULL};
	char ar;
	pid_t wt;
	pid_t child_pid;
	int i = 0;

	for (i = 0; i < 1; i++)
	{
		child_pid = fork();
		if (child_pid == 0)
		{
			ar =  execve(argv[0], argv, NULL);
			if (ar == -1)
			{
				perror("Error:");
			}
		}
		wt = wait(NULL);
		if (wt == -1)
		{
			perror("no funciono");
		}
	}
	return (0);
}
